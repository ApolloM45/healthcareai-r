p$debug = FALSE
p$varImp = TRUE
p$printResults = TRUE
p$cores = 1

# Run RandomForest
rf <- RandomForest$new(p)
rf$run()

# Run Lasso
lasso <- Lasso$new(p)
lasso$run()


#Plot ROCs from both supervised model classes
